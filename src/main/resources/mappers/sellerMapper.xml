<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.project.www.repository.SellerMapper">
    
    <insert id="register">
        INSERT INTO seller
        VALUES (#{id}, #{pw}, #{shopName})
    </insert>

    <select id="getList" resultType="com.project.www.domain.ProductVO">
        SELECT * FROM product
        WHERE
        id = #{id}
    </select>
    
</mapper>